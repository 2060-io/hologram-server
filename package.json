{
  "name": "hologram-server",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "description": "A server for Hologram",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nest build && tsc-alias -p tsconfig.build.json",
    "check-format": "biome check --unsafe",
    "fix-format": "biome check --write --unsafe",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "start": "node dist/main.js",
    "start:dev": "pnpm start --watch",
    "test": "vitest",
    "validate": "pnpm types:check && pnpm style:check",
    "prepare": "corepack enable"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.6",
    "firebase-admin": "^13.6.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/testing": "^11.1.13",
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "testcontainers": "^11.11.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.3.0",
    "vitest": "^1.4.0"
  },
  "packageManager": "pnpm@9.15.3"
}
